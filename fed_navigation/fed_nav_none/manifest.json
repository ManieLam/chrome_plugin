{
    "name": "前端导航",
    "version": "1.0.1",
    "manifest_version": 2,
    "content_scripts": [{
        "run_at": "document_idle",
        "matches": ["http://*/*", "https://*/*"],
        "js": ["bg.js"]
    }],
    "description": "前端设计技术导航",
    "browser_action": {
        "default_icon": "icon.gif",
        "default_title": "fed_nav",
        "default_popup": "index.html"
    }
}
